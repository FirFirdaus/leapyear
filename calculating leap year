year = int(input("Which year do you want to check? "))

if year % 4 == 0:
  if year % 100 == 0:
    if year % 400 == 0:
      print("This is a leap year") 
    else:
      print("This is not a leap year")
  else:
    print("This is a leap year")
else:
  print("This is not leap year")

  #a year divisible by 4 with no remainder is a leap year
  #except if it is also divisible by 100 in that case is no longer a leap year
  #unless that year is also divisible by 400 than is a also a leap year

